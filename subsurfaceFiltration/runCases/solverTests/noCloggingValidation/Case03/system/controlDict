FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application     colloidFiltrationFoam;

startFrom       latestTime;
startTime       0;
stopAt          endTime;

endTime	        6000; 
writeControl    timeStep;
writeInterval   20; 

deltaT          28.57;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      fixed;

timePrecision   3;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           1.0;
debug           1;

// To plot breakthrough curve

functions
    {
        #includeFunc  boundaryProbes
        #includeFunc  boundaryProbeTop
    };


// ************************************************************************* //
