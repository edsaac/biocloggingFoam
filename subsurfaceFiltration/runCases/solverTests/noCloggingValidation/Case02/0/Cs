FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Cs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(front|back|right|left)"
    {
        type    empty;
    }

    "(top|bottom)"
    {
        type    zeroGradient;
    }
}

